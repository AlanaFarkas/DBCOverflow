<div class="question-upvote">

  <form id="upvote-button-form-question" method="post" action="/questions/<%= question.id %>/upvote">
  <button class="vote-button" type="submit" name="submit_param" value="submit_value">cheers</button>
  </form>
  <p id="question-vote-count"><%= question.points%></p>
  <form id="downvote-button-form-question" method="post" action="/questions/<%= question.id %>/downvote">
    <button class="vote-button" type="submit" name="submit_param" value="submit_value">jeers</button>
  </form>

</div>

